{
    "name": "neimheadh/bank-middleware",
    "description": "Bank middleware API",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Mathieu Wambre",
            "email": "contact@neimheadh.fr"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "require": {
        "symfony/skeleton": "^6.2",
        "symfony/flex": "^2.2",
        "api-platform/core": "^3.0",
        "nelmio/cors-bundle": "^2.2",
        "symfony/asset": "^6.2",
        "symfony/expression-language": "^6.2",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "symfony/security-bundle": "^6.2",
        "doctrine/annotations": "^1.0",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.15",
        "symfony/property-access": "^6.2",
        "symfony/property-info": "^6.2",
        "symfony/serializer": "^6.2",
        "symfony/twig-bundle": "^6.2",
        "symfony/validator": "^6.2",
        "symfony/runtime": "^6.2",
        "symfony/yaml": "^6.2",
        "symfony/dotenv": "^6.2",
        "symfony/monolog-bundle": "^3.8",
        "sonata-project/admin-bundle": "^4.21",
        "sonata-project/doctrine-orm-admin-bundle": "^4.9",
        "sonata-project/form-extensions": "^1.18",
        "symfony/webpack-encore-bundle": "^1.16",
        "sonata-project/user-bundle": "^5.4",
        "symfony/acl-bundle": "^2.3"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "docker": true,
            "allow-contrib": true
        }
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.48"
    }
}
